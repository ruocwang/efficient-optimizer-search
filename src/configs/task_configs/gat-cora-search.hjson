{
    // model
    "model": "CRGNN",
    "criterion": "CrossEntropyLoss",
    // grid search
    "hps": {
        "lr": [0.0006, 0.001, 0.003, 0.006, 0.01, 0.03, 0.06],
    },
    "hierarchical": false,
    // eval
    "metric_name": "final_valid_acc",
    // algo
    "momentum": 0.9,
    "momentum2": 0.999,
    "momentum3": 0.999,


