"num_seeds_gs": 1,
"num_seeds": 4,
'num_epochs_gs': 1000,
'num_epochs': 1000,
'dataset': 'cora',
'weight_decay': 0.0005
// 'lr': 0.002,

'alpha': 0.2,
'dropout': 0.6,
'fastmode': false,
'hidden': 8,
'nb_heads': 8,
'patience': 100,
'sparse': false,
}
