{
    // model
    "model": "CRGNN",
    "criterion": "CrossEntropyLoss",
    // grid search
    "hps": {
        "lr": [0.2, 0.25, 0.35, 0.4, 0.45],
    },
    // "hierarchical": true,
    // eval
    "metric_name": "final_valid_acc",
    "num_seeds": 1,
    // algo
    "momentum": 0.9,
    "momentum2": 0.999,
    "momentum3": 0.999,


// gnn args modifiable
'warm_up': 100,
'num_epochs_gs': 300,
'num_epochs': 300,
'num_epochs_decay_max': -1, // full training epochs ()
// 'stages': [300],
'batch_size': 50000,
'batch_size_first': 50000,
'eval_every': 10,
// 'lr': 0.001,

// gnn args
'act': 'relu',
'adap': true,
'alpha': 0.5,
'att_drop': 0.4,
'bns': false,
'consis': false,
'dataset': 'products',
'decay': 0.9,
'down': 0.75,
'dropout': 0.5,
'ema': true,
'ema_decay': 0.99,
'focal': 'first',
'gama': 0.5,
'gap': 20,
'giant': true,
'gpu': 0,
'hidden': 256,
'input_drop': 0.2,
'kl': true,
'kl_lam': 0.02,
'label_drop': 0.5,
'label_mlp_layer': 4,
'label_num_hops': 9,
'label_residual': false,
'lam': 0.5,
'mean_teacher': true,
'method': 'SAGN',
'mlp_layer': 1,
'n_layers_1': 4,
'n_layers_2': 4,
'n_layers_3': 4,
'num_heads': 1,
'num_hops': 3,
'num_runs': 1,
'patience': 300,
'position_emb': false,
'pre_dropout': false,
'pre_process': true,
'residual': true,
'root': './data/gnn',
'seed': 0,
'sup_lam': 1.0,
'tem': 0.5,
'temp': 1,
'threshold': 0.8,
'top': 0.85,
// 'train_num_epochs': [0],
'use_emb': true,
'use_rlu': true,
'weight_decay': 0,
'weight_style': 'attention',
'zero_inits': true

}
